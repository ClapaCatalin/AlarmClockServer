<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE node PUBLIC
    "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
    "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd" >
<node xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd">
  <interface name="com.time.service.alarmClock">
      <method name="setTime">
      <arg name="time" direction="in" type="s">
      </arg>
        <doc:doc>
          <doc:description>
            <doc:para>
              set clock time
            </doc:para>
          </doc:description>
        </doc:doc>
      </method>
<method name="setAlarmTime">
      <arg name="alarmtime" direction="in" type="s">
      </arg>
        <doc:doc>
          <doc:description>
            <doc:para>
              set alarm time
            </doc:para>
          </doc:description>
        </doc:doc>
      </method>
<method name="getAlarmStatus">
      <arg name="alarmstatus" direction="out" type="b">
      </arg>
        <doc:doc>
          <doc:description>
            <doc:para>
              get alarm status
            </doc:para>
          </doc:description>
        </doc:doc>
      </method>
<method name="setAlarmStatus">
      <arg name="alarmstatus" direction="in" type="b">
      </arg>
        <doc:doc>
          <doc:description>
            <doc:para>
              set alarm status
            </doc:para>
          </doc:description>
        </doc:doc>
      </method>
<signal name="ringAlarm">
	<arg name="alarmring" type="s"/>
</signal>
</interface>
</node>
